name=pyparsing
version=2.0.3
release=1
source=(https://pypi.python.org/packages/source/p/pyparsing/pyparsing-$version.tar.gz)

build() {

  cd $name-$version
  /usr/bin/python setup.py install --prefix=/usr --root=$PKG

}
